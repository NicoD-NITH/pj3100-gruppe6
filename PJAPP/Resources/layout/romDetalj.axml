<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="#F9F7EE"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:minWidth="25px"
    android:minHeight="25px"
    android:weightSum="100">
    <include
        layout="@layout/menuBar" />
    <TextView
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="15"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/infoBoxStyle"
        android:id="@+id/romNavn"
        android:textSize="40sp"
        android:textColor="#000" />
    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_weight="50"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/infoBoxStyle"
        android:id="@+id/linearLayout1">
        <TextView
            android:text="Plasser:"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:id="@+id/plasser1"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="8dp"
            android:layout_alignParentTop="true"
            android:textColor="#000"
            android:textSize="20sp"
            android:gravity="left" />
        <TextView
            android:text="Prosjektor: "
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="8dp"
            android:id="@+id/prosjektor1"
            android:layout_below="@id/plasser1"
            android:textColor="#000"
            android:textSize="20sp"
            android:gravity="left" />
        <TextView
            android:text="Kan reserveres:"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:id="@+id/reserver1"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="8dp"
            android:layout_below="@id/prosjektor1"
            android:textColor="#000"
            android:textSize="20sp"
            android:gravity="left" />
        <TextView
            android:text="Reservasjons info:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/reservert1"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="8dp"
            android:layout_below="@id/reserver1"
            android:textColor="#000"
            android:textSize="20sp"
            android:gravity="left" />
        <TextView
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"
            android:id="@+id/plasser2"
            android:layout_toRightOf="@id/plasser1"
            android:textColor="#000"
            android:textSize="20sp"
            android:gravity="left" />
        <TextView
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"
            android:id="@+id/prosjektor2"
            android:layout_toRightOf="@id/prosjektor1"
            android:layout_below="@id/plasser2"
            android:textColor="#000"
            android:textSize="20sp"
            android:gravity="center" />
        <TextView
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"
            android:id="@+id/reserver2"
            android:layout_toRightOf="@id/reserver1"
            android:layout_below="@id/prosjektor2"
            android:textColor="#000"
            android:textSize="20sp"
            android:gravity="left" />
        <TextView
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="5dp"
            android:id="@+id/reservert2"
            android:layout_below="@id/reservert1"
            android:textColor="#000"
            android:textSize="20sp"
            android:gravity="left" />
    </RelativeLayout>
    <Spinner
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/romValg"
        android:visibility="gone"
        android:layout_weight="10"
        android:textColor="#000"
        android:textSize="10sp"
        android:textStyle="bold"
        android:background="@drawable/bigButtonStyle"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp" />
    <Button
        android:text="Reserver"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/ReserverButton"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_weight="20"
        android:background="@drawable/bigButtonStyle"
        android:textColor="#000"
        android:textSize="30sp"
        android:textStyle="bold" />
</LinearLayout>